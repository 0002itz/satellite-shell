const { id } = Astro.params
console.log({ id })

let launch 
if (id) {
launch = await getLaunchBy ({ id })
}
export async function getStaticPaths() {
    const launches = await getLatesLauches()
    return launches.map((launch) => ({
        params : { id: launch.id},
    }))
}

